{
  "version": 2,
  "builds": [
    { 
      "src": "src/main.ts", 
      "use": "@vercel/node", 
      "config": {
        "includeFiles": [
          "node_modules/swagger-ui-dist/**/*"
        ]
      } 
    }
  ],
  "routes": [
    {
      "src": "/swagger",
      "dest": "/node_modules/swagger-ui-dist/$1"
    },
    {
      "src": "/(.*)", 
      "dest": "/src/main.ts"
    }
  ]
}
